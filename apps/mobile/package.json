{
	"name": "@sportapp/mobile",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "yarn lint:js",
		"lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"start": "react-native start",
		"test": "jest --ci --reporters=default --config jest.config.ts --no-cache --coverage --forceExit --detectOpenHandles --logHeapUsage",
		"pre-commit": "lint-staged",
		"format": "prettier --write \"**/*.{ts,tsx,md,js}\""
	},
	"dependencies": {
		"@sportapp/langs": "*",
		"i18next": "^23.10.1",
		"react": "18.2.0",
		"react-i18next": "^14.1.0",
		"react-native": "0.73.6",
		"react-native-paper": "^5.12.3",
		"react-native-safe-area-context": "^4.9.0",
		"react-native-vector-icons": "^10.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native/babel-preset": "0.73.21",
		"@react-native/eslint-config": "0.73.2",
		"@react-native/metro-config": "0.73.5",
		"@react-native/typescript-config": "0.73.1",
		"@sportapp/eslint-config": "*",
		"@sportapp/prettier-config": "*",
		"@types/jest": "^29.5.12",
		"@types/react": "^18.2.6",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.6.3",
		"eslint": "^8.19.0",
		"jest": "^29.6.3",
		"lint-staged": "^15.2.2",
		"prettier": "2.8.8",
		"react-test-renderer": "18.2.0",
		"ts-jest": "^29.1.2",
		"ts-jest-mock-import-meta": "^1.2.0",
		"ts-node": "^10.9.2",
		"typescript": "5.0.4"
	},
	"engines": {
		"node": ">=18"
	}
}
