{
	"name": "@sportapp/web",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"dev": "vite",
		"lint": "yarn lint:js && yarn lint:css",
		"lint:css": "stylelint \"src/**/*.{css,scss}\" --max-warnings 0",
		"lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix && stylelint \"src/**/*.{css,scss}\" --max-warnings 0 --fix",
		"lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"pre-commit": "lint-staged",
		"preview": "vite preview",
		"test": "jest --ci --reporters=default --config jest.config.ts --no-cache --coverage --forceExit --detectOpenHandles --logHeapUsage --silent=false --verbose"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.0",
		"@fontsource/roboto": "^5.0.12",
		"@hookform/resolvers": "^3.3.4",
		"@mui/icons-material": "^5.15.14",
		"@mui/lab": "^5.0.0-alpha.170",
		"@mui/material": "^5.15.14",
		"@mui/x-date-pickers": "^7.1.0",
		"@sportapp/langs": "*",
		"@sportapp/stores": "*",
		"country-state-city": "^3.2.1",
		"date-fns": "^3.6.0",
		"i18next": "^23.10.1",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.51.2",
		"react-i18next": "^14.1.0",
		"react-router-dom": "^6.22.3",
		"sort-by": "^1.2.0",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.24.1",
		"@sportapp/eslint-config": "*",
		"@sportapp/prettier-config": "*",
		"@sportapp/stylelint-config": "*",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^14.2.2",
		"@types/jest": "^29.5.12",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.19",
		"babel-jest": "^29.7.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss": "^8.4.38",
		"sass": "^1.72.0",
		"stylelint": "^16.2.1",
		"stylelint-config-standard-scss": "^13.0.0",
		"tailwindcss": "^3.4.3",
		"ts-jest": "^29.1.2",
		"ts-jest-mock-import-meta": "^1.2.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	},
	"peerDependencies": {
		"react": "^17.0.0 || ^18.0.0",
		"react-dom": "^17.0.0 || ^18.0.0"
	}
